fun fib(n: num): num {
  if (n == 0) {
    return 0
  } else if (n == 1) {
    return 1
  } else {
    return fib(n - 1) + fib(n - 2)
  }
}

fun fibt(n0, n1, c){
  if (c == 0) {
    return n0
  }
  else if (c == 1) {
    return n1
  }
  return fibt(n1, n0+n1, c-1)
}
  
fun fibn(n) {
  fibt(0, 1, n)
}


fun main {
  var before = System.now
  print(before)
	print('result:', fib(30))
  var after = System.now
  print(after)
  print('time: ', (after - before) / 1000)
}
